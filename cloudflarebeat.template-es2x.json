            },
            "sslConnectionId": {"type": "string", "index": "not_analyzed", "ignore_above": 256},
            "uri": {
              "type": "string", 
              "ignore_above": 1024,
              "fields": {
                "raw": {
                  "index": "not_analyzed",
                  "type": "string",
                  "ignore_above": 1024
                }
              }
            },
            "userAgent": {
              "type": "string", 
