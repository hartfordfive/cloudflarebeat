        },
        "edgeRequest": {
          "properties": {
            "bodyBytes": {"type": "long"},
            "bytes": {"type": "long"},
            "headers": {"type": "nested"},
            "httpHost": {
              "type": "string", 
              "ignore_above": 256,
              "fields": {
                "raw": {
                  "index": "not_analyzed",
                  "type": "string",
                  "ignore_above": 1024
                }
              }
            },
            "httpMethod": {"type": "string", "index": "not_analyzed", "ignore_above": 256},
            "keepaliveStatus": {"type": "string", "index": "not_analyzed", "ignore_above": 256},
            "uri": {
              "type": "string", 
              "ignore_above": 1024,
              "fields": {
                "raw": {
                  "index": "not_analyzed",
                  "type": "string",
                  "ignore_above": 1024
